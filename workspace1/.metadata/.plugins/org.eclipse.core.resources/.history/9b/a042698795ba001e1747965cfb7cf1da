package hacker_rank;
import java.util.Scanner;

public class Palandrom_String {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 String sr ="dad";
		 String rev="";
		 int length = sr.length();
		 for(int i =sr.length()-1;i>=0;i--)
		 {
			 rev = rev+sr.charAt(i);
		 }
		 System.out.println(rev);
		 sr.compareTo(rev);
		 
		}

	}
	

