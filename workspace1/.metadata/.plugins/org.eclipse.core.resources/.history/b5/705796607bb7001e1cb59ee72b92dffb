package hacker_rank;
import java.util.Scanner;

public class Rank {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		  Scanner sc=new Scanner(System.in);
          System.out.println("================================");
          for(int i=0;i<3;i++){
              String s1=sc.next();
              int x=sc.nextInt();
              //Complete this line
              System.out.print(""+s1);
              System.out.println(""     +x);
          }
          System.out.println("================================");

	}

}
